{
    "name": "takethelead/laravel-settings",
    "license": "proprietary",
    "description": "Manage config settings through the database",
    "homepage": "https://github.com/takethelead/laravel-settings",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/config": "^8.2|^9.0",
        "illuminate/database": "^8.2|^9.0",
        "illuminate/events": "^8.2|^9.0",
        "illuminate/validation": "^8.2|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.20|^7.0",
        "symfony/var-dumper": "^5.0|^6.0",
        "phpunit/phpunit": "^9.5.10",
        "phpro/grumphp": "^1.8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "TakeTheLead\\Settings\\": "src",
            "TakeTheLead\\Settings\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TakeTheLead\\Settings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TakeTheLead\\Settings\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
